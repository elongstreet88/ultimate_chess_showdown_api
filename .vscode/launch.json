{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug from Local (debug config)",
      "type": "debugpy",
      "request": "launch",
      "module": "app",
      "cwd": "${workspaceFolder}",
      "justMyCode": false,
      "envFile": "${workspaceFolder}/.env",
    },
    {
      "name": "Python: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}
